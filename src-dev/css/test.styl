*
  margin 0
  padding 0
body 
  background FFF
  width 100%
  height 100%
  color 555
  font-size 14px
  font-family Helvetica, Arial, "Hiragino Sans GB", sans-serif
h1
  word-break: break-all
  font-size: 32px
  font-weight: bold
  line-height: 1.5
  text-align center
  padding 15px 0
  margin-bottom 20px 
  border-bottom 2px dashed e7e7e7
table,tr,th,td
  border-spacing 0
  cellspacing 0
  padding 0
.wrap
  width 1200px 
  margin 0 auto
td 
  border 1px solid e7e7e7
  border-spacing 0
  padding 0
  width 70px 
  height 60px
  text-align center
p 
  position relative
  padding-top 40px
  height 20px 
  line-height 20px 
  &:before
    position absolute
    left 0 
    top 0
    display inline-block
    width 100%
    height 40px 
    line-height 40px
    font-size 30px
    color #333
foo(v1) 
  for v2 in 1 2 3 4 5 6 7 8 9 A B C D E F
    v3 = v1 + v2
    v4 = '\21' + v3
    .u21{v3}:before
      content v4 
foo(A)


